  integer, target, copointer :: tp3_b(:)
  integer, target, copointer :: tp1_b1, tp1_b2
  integer, target, copointer, dimension(:) :: tp2_b1, tp2_b2
  integer, target, copointer :: tp3_b1(:), tp3_b2(:)
