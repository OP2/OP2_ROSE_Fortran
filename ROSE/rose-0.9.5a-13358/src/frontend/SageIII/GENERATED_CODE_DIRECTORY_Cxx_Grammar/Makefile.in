# JJW (2/25/2008): set a flag (only used when running ROSE applications) to
# have them run using the build tree rather than the install tree -- this is
# important for "make check" in an uninstalled copy of ROSE.
@USE_ROSE_IN_BUILD_TREE_VAR_TRUE@export ROSE_IN_BUILD_TREE=$(top_builddir)

# Use a makefile include meachanism.  However I can't make this work!
# include $(top_builddir)/src/frontend/SageIII/GENERATED_CODE_DIRECTORY_Cxx_Grammar/generatedFileList.inc
# GENERATED_SOURCE = SgAddOp.C

@ROSE_USE_SMALLER_GENERATED_FILES_TRUE@haveRosettaGeneratedSource:
@ROSE_USE_SMALLER_GENERATED_FILES_TRUE@	cd $(top_builddir)/src/ROSETTA/src; $(MAKE)
@ROSE_USE_SMALLER_GENERATED_FILES_TRUE@	touch haveRosettaGeneratedSource

# Force ROSETTA to be run to generate the ROSE IR source code.
@ROSE_USE_SMALLER_GENERATED_FILES_TRUE@force-haveRosettaGeneratedSource:
@ROSE_USE_SMALLER_GENERATED_FILES_TRUE@	-rm *.h *.C
@ROSE_USE_SMALLER_GENERATED_FILES_TRUE@	$(MAKE) haveRosettaGeneratedSource

@ROSE_USE_SMALLER_GENERATED_FILES_TRUE@$(GENERATED_SOURCE): haveRosettaGeneratedSource
# nodist_libgeneratedIR_la_SOURCES = $(GENERATED_SOURCE)

# DQ (10/18/2007): Added AST_FILE_IO.h StorageClasses.h
# Removed sla.h (redundant with location in src/util/commandlineProcessing/sla.h)
# include_HEADERS = *.h

test:
	echo "Force ROSETTA to regenerate the source code for the ROSE IR."
	cd $(top_builddir)/src/ROSETTA/src; $(MAKE)

clean-local:
#	rm -rf libgeneratedIR.la
	rm -rf *.h *.C *.loT

printFiles:
	echo "GENERATED_SOURCE = $(GENERATED_SOURCE)"

printObjFiles:
	echo "GENERATED_OBJ = $(GENERATED_OBJ)"

distclean-local:
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
