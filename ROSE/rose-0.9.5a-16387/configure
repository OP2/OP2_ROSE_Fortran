    # For Intel turn on 64bit migration/portability warnings
      C_WARNINGS="-w1 -Wall -Wcheck -Wp64"
      ;;
    "KCC --c" | mpKCC)
      C_WARNINGS="--for_init_diff_warning --new_for_init -w"
      ;;
    cc)
      case $host_os in
        solaris*| sun4*)
          C_WARNINGS=""
    esac
    ;;
  esac
else
  C_WARNINGS=$with_C_WARNINGS
