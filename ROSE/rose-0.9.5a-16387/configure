_ACEOF

	;;
fortran)
	if test "x$USE_JAVA" = x1; then
	  if test "x$GFORTRAN_PATH" = "x"; then
            { { echo "$as_me:$LINENO: error: [Fortran support] gfortran not found: required for syntax checking and semantic analysis.
                           Do you need to explicitly specify gfortran using the \"--with-gfortran=path/to/gfortran\" configure-option? (See ./configure --help)
See \`config.log' for more details." >&5
echo "$as_me: error: [Fortran support] gfortran not found: required for syntax checking and semantic analysis.
                           Do you need to explicitly specify gfortran using the \"--with-gfortran=path/to/gfortran\" configure-option? (See ./configure --help)
See \`config.log' for more details." >&2;}
   { (exit 1); exit 1; }; }
          else
     	    support_fortran_language=yes

cat >>confdefs.h <<\_ACEOF
#define ROSE_BUILD_FORTRAN_LANGUAGE_SUPPORT
_ACEOF

          fi
        elif test "x$with_java" = "xno" ; then
	  { { echo "$as_me:$LINENO: error: [Fortran support] can't support the Fortran language because you specified --with-java='$with_java'. You can turn off Fortran support with --disable-fortran (See ./configure --help)
See \`config.log' for more details." >&5
