				if ($CC $ac_cv_enable_i386 $CFLAGS -o $RSI386_TEMP $RSI386_TEMP.c &&
		    $RSI386_TEMP &&
		    file $RSI386_TEMP |grep 32-bit |grep -q Intel); then
			: all ok
		else
			echo "$as_me:$LINENO: result: $ac_cv_enable_i386" >&5
echo "${ECHO_T}$ac_cv_enable_i386" >&6
			{ { echo "$as_me:$LINENO: error: User-specified --enable-i386 value ($ac_cv_enable_i386) does not work properly." >&5
echo "$as_me: error: User-specified --enable-i386 value ($ac_cv_enable_i386) does not work properly." >&2;}
   { (exit 1); exit 1; }; }
		fi
	fi

	echo "$as_me:$LINENO: result: $ac_cv_enable_i386" >&5
