						ac_cv_enable_i386=no

				if (test "$ac_cv_enable_i386" = no &&
		    $CC $CFLAGS -o $RSI386_TEMP $RSI386_TEMP.c &&
		    $RSI386_TEMP &&
		    file $RSI386_TEMP |grep 32-bit |grep -q Intel); then
			$ac_cv_enable_i386=
		fi

				if (test "$ac_cv_enable_i386" = no &&
		    $CC -m32 $CFLAGS -o $RSI386_TEMP $RSI386_TEMP.c &&
		    $RSI386_TEMP &&
                    file $RSI386_TEMP |grep 32-bit |grep -q Intel); then
			ac_cv_enable_i386="-m32"
		fi

				if test "$RSI386_SPECIFIED" = "yes" -a "$ac_cv_enable_i386" = "no"; then
			{ { echo "$as_me:$LINENO: error: The --enable-i386 was specified but common switches to produce such a file don't work" >&5
echo "$as_me: error: The --enable-i386 was specified but common switches to produce such a file don't work" >&2;}
   { (exit 1); exit 1; }; }
		fi

