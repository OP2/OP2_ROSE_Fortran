@ROSE_USE_INTEL_PIN_TRUE@	$(VALGRIND) ./staticBinaryAnalysis $(ROSE_FLAGS) -c $(srcdir)/$(@:.o=.C)

@ROSE_USE_INTEL_PIN_TRUE@copyFiles:
@ROSE_USE_INTEL_PIN_TRUE@	cp $(srcdir)/*.h $(top_srcdir)/tests/CompilerTests/Pin_tests
@ROSE_USE_INTEL_PIN_TRUE@	cp $(srcdir)/*.C $(top_srcdir)/tests/CompilerTests/Pin_tests

@ROSE_USE_INTEL_PIN_TRUE@check-local:
@ROSE_USE_INTEL_PIN_TRUE@	@echo "Development tests for Intel Pin tests."
#  Run this test explicitly since it has to be run using a specific rule and can't be lumped with the rest
#	These C programs must be called externally to the test codes in the "TESTCODES" make variable
#	@$(MAKE) $(PASSING_TEST_Objects)
#	@$(MAKE) testMemcopy testGraphTrace testImageload testDynamicBinaryInfo
@ROSE_USE_INTEL_PIN_TRUE@	@$(MAKE) $(ROSE_PIN_TESTS)
@ROSE_USE_INTEL_PIN_TRUE@	@$(MAKE) testStaticBinaryAnalysis
@ROSE_USE_INTEL_PIN_TRUE@	@$(MAKE) testStaticDynamicBinaryAnalysis
@ROSE_USE_INTEL_PIN_TRUE@	@$(MAKE) testDynamicStaticBinaryAnalysis
@ROSE_USE_INTEL_PIN_TRUE@	@echo "********************************************************************************************************"
@ROSE_USE_INTEL_PIN_TRUE@	@echo "****** ROSE/tests/roseTests/binaryTests/Pin_tests: make check rule complete (terminated normally) ******"
@ROSE_USE_INTEL_PIN_TRUE@	@echo "********************************************************************************************************"

@ROSE_USE_INTEL_PIN_FALSE@check-local:
@ROSE_USE_INTEL_PIN_FALSE@	@echo "Intel Pin development tests NOT run.  To run these reconfigure with:"
@ROSE_USE_INTEL_PIN_FALSE@	@echo "*******************************************************************************************************"
@ROSE_USE_INTEL_PIN_FALSE@	@echo "*************** make check not run here: configure --with-IntelPin=<path to Intel Pin> ****************"
@ROSE_USE_INTEL_PIN_FALSE@	@echo "*******************************************************************************************************"

clean-local:
	rm -f *.o rose_*.[s] *.dot *.pdf *~ *.ps *.out *.new *.dump rose_performance_report_lockfile.lock *.so
	rm -f pinRoseTranslator.so  input_testcode pin.log
	rm -rf QMTest

print:
	@echo "INTEL_PIN_WITH_DWARF_LIBS_PATH = $(INTEL_PIN_WITH_DWARF_LIBS_PATH)"
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
